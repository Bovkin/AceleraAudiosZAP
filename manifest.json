{
    "manifest_version": 2,
    "name": "AceleraZAP",
    "version": "0.1",
    "description": "Acelerando audios do Whatsapp",
    "content_scripts": [
        {
            "matches": ["https://web.whatsapp.com/"],
            "css": ["style.css"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    }
}
